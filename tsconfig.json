{
  "compilerOptions": {
    "declaration": false,
    "module": "commonjs",
    "lib": ["es2015", "es2016", "es2017", "esnext.asynciterable"],
    "target": "es2015",
    "moduleResolution": "node",
    "sourceMap": false,
    "noImplicitAny": true,
    "outDir": "./bin",
    "removeComments": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*.ts"]
}
