{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs",
    "lib": ["es2015", "es2016", "es2017", "esnext.asynciterable"],
    "target": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": true,
    "outDir": ".",
    "removeComments": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
