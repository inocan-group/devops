{
  "name": "do-devops",
  "version": "0.4.1",
  "description": "Devops commands for your nodejs projects",
  "main": "./bin/index.js",
  "typings": "./bin/index.map.ts",
  "bin": {
    "do": "./bin/do.js"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "clean": "rimraf src/**/*.js && rimraf bin",
    "test": "mocha -r ts-node/register test/**/*-spec.ts",
    "build": "tsc && chmod +x bin/do.js",
    "watch": "tsc -w",
    "do": "./bin/do.js",
    "visualize": "yarn madge src --extensions ts -i /tmp/dep-ts-visualize.png && open /tmp/dep-ts-visualize.png",
    "visualize:js": "yarn madge bin --extensions js -i /tmp/dep-js-visualize.png && open /tmp/dep-js-visualize.png",
    "circular": "yarn madge src --extensions ts --circular",
    "orphans": "yarn madge src --extensions ts --orphans",
    "leaves": "yarn madge src --extensions ts --leaves",
    "warnings": "yarn madge src --extensions ts --warnings",
    "summary": "yarn madge src --extensions ts --summary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inocan-group/devops.git"
  },
  "keywords": [
    "devops",
    "nodejs",
    "serverless"
  ],
  "author": "Ken Snyder",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inocan-group/devops/issues"
  },
  "homepage": "https://github.com/inocan-group/devops#readme",
  "dependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "ast-types": "^0.13.3",
    "async-shelljs": "^0.1.3",
    "aws-sdk": "^2.658.0",
    "aws-ssm": "^0.3.11",
    "babylon": "^7.0.0-beta.47",
    "bestzip": "^2.1.5",
    "chalk": "^4.0.0",
    "chokidar": "^3.4.0",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "common-types": "^1.15.7",
    "date-fns": "^2.14.0",
    "fast-glob": "^3.2.2",
    "globby": "^11.0.0",
    "inquirer": "^6.5.1",
    "js-yaml": "^3.13.1",
    "lodash.get": "^4.4.2",
    "matcher": "^2.0.0",
    "play-sound": "^1.1.3",
    "recast": "^0.19.0",
    "simple-git": "^2.12.0",
    "table": "^5.4.6",
    "typed-mapper": "^0.2.0",
    "webpack": "^4.43.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/chai": "^4.2.11",
    "@types/chalk": "^2.2.0",
    "@types/chokidar": "^2.1.3",
    "@types/globby": "^9.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/js-yaml": "^3.12.3",
    "@types/lodash.get": "^4.4.6",
    "@types/matcher": "^2.0.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^10.8.0",
    "@types/table": "^5.0.0",
    "@types/webpack": "^4.41.11",
    "chai": "^4.2.0",
    "madge": "^3.9.1",
    "mocha": "^7.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  }
}